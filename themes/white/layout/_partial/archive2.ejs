<section class="archive-outer">
    <section class="archive-inner">
        <% var last; %>
        <% site.posts.sort('date', -1).each(function(post, i) { %>
        <% var year = post.date.year(); %>
        <% if (last != year){ %>
            <% if (last != null){ %>
            <% } %>
            <% last = year; %>
        <div class="archive-name animate__animated  animate__fadeInUp"><%=  year %></div>
        <% } %>
          <a  href="<%=url_for(post.path)%>">
          <div class="archive-post animate__animated  animate__fadeInUp">
            <div class="archive-post-title"><%=post.title%></div>
            <div class="archive-post-date"><%=date(post.date, 'DD / MM') %></div>
          </div>
          </a>
        <% }) %>
    </section>
</section>
